<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
	<item android:state_pressed="true" >
		<shape android:shape="rectangle" >
			<corners android:radius="@dimen/corner_radius_large_outline_button" />
			<solid android:color="@color/medium_blue_40pc" />
			<stroke
				android:color="?attr/PrimaryThemeColorDark"
				android:width="@dimen/stroke_width_outline_button" />
		</shape>
	</item>
	<item android:state_focused="true" >
		<shape android:shape="rectangle" >
			<corners android:radius="@dimen/corner_radius_large_outline_button" />
			<solid android:color="@color/medium_blue_40pc"/>
			<stroke
				android:color="?attr/PrimaryThemeColorDark"
				android:width="@dimen/stroke_width_outline_button" />
		</shape>
	</item>
	<item>
		<shape android:shape="rectangle" >
			<corners android:radius="@dimen/corner_radius_large_outline_button" />
			<solid android:color="@android:color/transparent" />
			<stroke
				android:color="?attr/PrimaryThemeColorDark"
				android:width="@dimen/stroke_width_outline_button" />
		</shape>
	</item>
</selector>
